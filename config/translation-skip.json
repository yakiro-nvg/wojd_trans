{
  "rules": [
    {
      "namespace": "RowObjectBase",
      "keyRegex": ".*-Title$",
      "sourcePattern": "^[A-Za-z0-9_]+$",
      "comment": "Skip Pinyin lookup keys like Paiming, Bangpai - but allow Chinese titles"
    },
    {
      "namespace": "Battle",
      "keyRegex": "^(13[0-9]|14[0-9]|15[0-2])$"
    },
    {
      "namespace": "Battle.txt",
      "keyRegex": "^(1[3-5][0-9]|152)$"
    }
  ]
}
